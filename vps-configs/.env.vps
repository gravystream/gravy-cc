# /var/www/gravy-cc/.env on VPS
# Fill in REPLACE_ME values before running deploy

DATABASE_URL="postgresql://gravy_user:REPLACE_ME_DB_PASSWORD@localhost:5432/gravy_cc"
NEXTAUTH_URL="https://novaclio.io"
NEXTAUTH_SECRET="REPLACE_ME_generate_with: openssl rand -base64 32"

# Anthropic API — get from https://console.anthropic.com
ANTHROPIC_API_KEY="REPLACE_ME"

# Paystack — get from https://dashboard.paystack.com/#/settings/developer
PAYSTACK_SECRET_KEY="REPLACE_ME"
PAYSTACK_PUBLIC_KEY="REPLACE_ME"
